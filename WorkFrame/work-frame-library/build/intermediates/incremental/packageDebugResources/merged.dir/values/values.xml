<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="integer" name="Max"/>
    <declare-styleable name="CalendarView">
        <attr format="color" name="mc_color"/>
        <attr format="color" name="mc_selectColor"/>
        <attr format="color" name="mc_selectTextColor"/>
        <attr format="color" name="mc_textColor"/>
        <attr format="boolean" name="mc_startsOnSunday"/>
    </declare-styleable>
    <declare-styleable name="CheckStart">
        <attr format="integer" name="Progress"/>
        <attr name="Max"/>
        <attr format="enum" name="StartGravity">
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
            <enum name="center" value="0"/>
            <enum name="top" value="3"/>
            <enum name="bottom" value="4"/>
        </attr>
        <attr format="enum" name="StartOrientation">
            <enum name="vertical" value="1"/>
            <enum name="horizontal" value="0"/>
        </attr>
        <attr format="enum" name="StartMode">
            <enum name="only" value="1"/>
            <enum name="which" value="2"/>
            <enum name="range" value="3"/>
            <enum name="begin" value="0"/>
        </attr>
        <attr format="dimension" name="StartMargin"/>
        <attr format="dimension" name="StartSize"/>
        <attr format="boolean" name="isChecked"/>
        <attr format="reference" name="CheckedDrawable"/>
        <attr format="reference" name="UncheckDrawable"/>
    </declare-styleable>
    <declare-styleable name="CircleLoadingHWView">
        <!--控件大小-->
        <attr format="dimension" name="view_size"/>
        <!--控件颜色-->
        <attr format="color" name="view_color"/>
        <!--转一圈花费时长-->
        <attr format="integer" name="circle_duration"/>
    </declare-styleable>
    <declare-styleable name="CustomProgress">
        <attr format="reference|string" name="pgText"/>
    </declare-styleable>
    <declare-styleable name="GifImageView">
        <!--gif文件引用-->
        <attr format="reference" name="gif_src"/>
        <!--是否加载完自动播放-->
        <attr format="boolean" name="auth_play"/>
        <!--播放次放，默认永远播放-->
        <attr format="integer" name="play_count"/>
        <!--播放完成后是否停留在最后一帧，默认false-->
        <attr format="boolean" name="end_last_frame"/>
    </declare-styleable>
    <declare-styleable name="ProgressShowView">
        <attr format="reference" name="ProgressBackground"/>

    </declare-styleable>
    <declare-styleable name="SanJiaoView">
        <attr format="dimension" name="sjv_resolutionWidth"/>
        <attr format="dimension" name="sjv_resolutionHeight"/>
        <attr format="color" name="sjv_color"/>
        <attr name="sjv_direction">
            <enum name="top" value="0"/>
            <enum name="bottom" value="1"/>
            <enum name="right" value="2"/>
            <enum name="left" value="3"/>
        </attr>
    </declare-styleable>
    <dimen name="line">1dp</dimen>
    <dimen name="padding_10">10dp</dimen>
    <dimen name="padding_15">15dp</dimen>
    <dimen name="padding_5">5dp</dimen>
    <dimen name="text_middle">14sp</dimen>
    <dimen name="text_mini">8sp</dimen>
    <dimen name="text_normal">16sp</dimen>
    <dimen name="text_small">11sp</dimen>
    <dimen name="text_title">18sp</dimen>
    <string name="app_name">work-frame-library</string>
    <string name="cancel">取消</string>
    <string name="change">变换</string>
    <string name="delete">删除</string>
    <string name="edit">编辑</string>
    <string name="hint_nocopy">不能复制粘贴的edittext</string>
    <string name="notifyMsg">缺少权限</string>
    <string name="notifyTitle">警告</string>
    <string name="setting">去设置</string>
    <string name="sure">确定</string>
    <string name="title_avLoading">AVLoadingIndicatorView</string>
    <string name="title_buttons">Buttons</string>
    <string name="title_text_avLoading">https://github.com/jzeferino/Xamarin.Android.AVLoadingIndicatorView</string>
</resources>